{
    "$schema": "https://schemas.wp.org/wp/6.6/theme.json",
    "version": 3,
    "settings": {
        "appearanceTools": true,
        "color": {
            "defaultPalette": false,
            "palette": [
                {
                    "slug": "edu-white",
                    "color": "#ffffff",
                    "name": "Edumi White"
                },
                {
                    "slug": "edu-gray-100",
                    "color": "rgb(243 244 246)",
                    "name": "Edumi Gray 100"
                },
                {
                    "slug": "edu-gray-200",
                    "color": "rgb(229 231 235)",
                    "name": "Edumi Gray 200"
                },
                {
                    "slug": "edu-gray-300",
                    "color": "rgb(209 213 219)",
                    "name": "Edumi Gray 300"
                },
                {
                    "slug": "edu-gray-400",
                    "color": "rgb(156 163 175)",
                    "name": "Edumi Gray 400"
                },
                {
                    "slug": "edu-gray-500",
                    "color": "rgb(107 114 128)",
                    "name": "Edumi Gray 500"
                },
                {
                    "slug": "edu-gray-600",
                    "color": "rgb(75 85 99)",
                    "name": "Edumi Gray 600"
                },
                {
                    "slug": "edu-gray-700",
                    "color": "rgb(55 65 81)",
                    "name": "Edumi Gray 700"
                },
                {
                    "slug": "edu-gray-800",
                    "color": "rgb(31 41 55)",
                    "name": "Edumi Gray 800"
                },
                {
                    "slug": "edu-gray-900",
                    "color": "rgb(17 24 39)",
                    "name": "Edumi Gray 900"
                },
                {
                    "slug": "edu-primary",
                    "color": "rgb(239 68 68)",
                    "name": "Edumi Primary"
                },
                {
                    "slug": "edu-accent",
                    "color": "rgb(67 56 202)",
                    "name": "Edumi Accent"
                },
                {
                    "slug": "edu-success",
                    "color": "rgb(52 211 153)",
                    "name": "Edumi Success"
                },
                {
                    "slug": "edu-info",
                    "color": "rgb(96 165 250)",
                    "name": "Edumi Info"
                },
                {
                    "slug": "edu-warning",
                    "color": "rgb(251 146 60)",
                    "name": "Edumi Warning"
                },
                {
                    "slug": "edu-danger",
                    "color": "rgb(236 72 153)",
                    "name": "Edumi Danger"
                }
            ],
            "background": true,
            "text": true,
            "link": true,
            "custom": true,
            "duotone": [
                {
                    "slug": "edu-pink-sunset",
                    "colors": [
                        "#11245e",
                        "#dc4379"
                    ],
                    "name": "Edumi Pink Sunset"
                },
                {
                    "slug": "edu-yellow-brown",
                    "colors": [
                        "#11245e",
                        "#FFFF00"
                    ],
                    "name": "Edumi Yellow Brown"
                }
            ],
            "defaultGradients": false,
            "customGradient": false,
            "gradients": [
                {
                    "slug": "edu-summer-dog",
                    "name": "Edumi Summer Dog",
                    "gradient": "linear-gradient(105deg, #a8ff78, #78ffd6)"
                },
                {
                    "slug": "edu-css-gradient",
                    "name": "Edumi CSS Gradient",
                    "gradient": "linear-gradient(135deg, #874633, #A61138)"
                }
            ]
        },
        "blocks": {
            "core/site-title": {
                "color": {
                    "palette": [
                        {
                            "slug": "edu-primary",
                            "color": "#ef4444",
                            "name": "Edumi Primary"
                        },
                        {
                            "slug": "edu-secondary",
                            "color": "#ECFAD5",
                            "name": "Edumi Secondary"
                        }
                    ],
                    "link": true,
                    "customGradient": false,
                    "gradients": []
                },
                "typography": {
                    "fontFamilies": [
                        {
                            "fontFamily": "Pacifico, sans-serif",
                            "slug": "edu-pacifico",
                            "name": "Edumi Pacifico"
                        }
                    ]
                }
            },
            "core/cover": {
                "color": {
                    "duotone": [],
                    "customDuotone": false
                }
            },
            "core/paragraph": {
                "color": {
                    "defaultGradients": true,
                    "customGradient": true
                }
            },
            "core/preformatted": {
                "typography": {
                    "fontSizes": [],
                    "customFontSize": false
                }
            }
        },
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "Rubik, sans-serif",
                    "slug": "edu-rubik",
                    "name": "Edumi Rubik"
                }
            ],
            "fontSizes": [
                {
                    "slug": "small",
                    "size": "0.75rem",
                    "name": "Small"
                },
                {
                    "slug": "medium",
                    "size": "1.25rem",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "2.25rem",
                    "name": "Large"
                },
                {
                    "slug": "x-large",
                    "size": "3rem",
                    "name": "Extra large"
                },
                {
                    "slug": "gigantic",
                    "size": "3.75rem",
                    "name": "Gigantic"
                }
            ],
            "lineHeight": true,
            "dropCap": true,
            "fontWeight": true,
            "fontStyle": true,
            "textTransform": true,
            "letterSpacing": true,
            "textDecoration": true
        },
        "layout": {
            "contentSize": "800px",
            "wideSize": "1100px"
        },
        "spacing": {
            "margin": true,
            "padding": true,
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ],
            "blockGap": true
        }
    },
    "styles": {
        "color": {
            "text": "var(--wp--preset--color--edu-gray-700)",
            "background": "var(--wp--preset--color--edu-white)"
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--edu-primary)"
                }
            }
        },
        "blocks": {
            "core/button": {
                "color": {
                    "gradient": "var(--wp--preset--gradient--edu-css-gradient)"
                }
            },
            "core/pullquote": {
                "border": {
                    "width": "2px",
                    "radius": "10px",
                    "style": "solid",
                    "color": "var(--wp--preset--color--edu-primary)"
                }
            },
            "core/site-title": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--edu-pacifico)"
                }
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--edu-rubik)",
            "fontSize": "16px",
            "fontStyle": "normal",
            "fontWeight": "normal",
            "lineHeight": "inherit",
            "textDecoration": "none",
            "textTransform": "none"
        },
        "spacing": {
            "margin": {
                "top": "0px",
                "bottom": "0px",
                "left": "0px",
                "right": "0px"
            },
            "padding": {
                "top": "0px",
                "bottom": "0px",
                "left": "0px",
                "right": "0px"
            },
            "blockGap": "4rem"
        }
    },
    "customTemplates": [
        {
            "name": "full-width-page",
            "title": "Full Width Page",
            "postTypes": [
                "page"
            ]
        },
        {
            "name": "affiliate-page",
            "title": "Affiliate Page",
            "postTypes": [
                "page"
            ]
        },
        {
            "name": "blank-page",
            "title": "Blank Page",
            "postTypes": [
                "page"
            ]
        }
    ]
}